<div class="list">

<table>

  <tr>
    <th>State</th>
    <th>Redux Reference</th>
    <th>Programme Rame</th>
    <th>Description</th>
    <th>Channel</th>
    <th>Date</th>
    <th>Duration</th>
    <th>In</th>
    <th>Out</th>
    <th>When</th>
    <th>Notes</th>
  </tr>

  [% FOREACH programme IN stash.list %]

    <tr>
      <td>[% programme.state | html %]</td>
      <td><a href="/p/[% programme.redux_reference | html %]">[% programme.redux_reference | html %]</a></td>
      <td>[% programme.programme_name | html %]</td>
      <td>[% programme.description | html %]</td>
      <td>[% programme.channel | html %]</td>
      <td>[% programme.date | html %]</td>
      <td>[% programme.duration | html %]</td>
      <td>[% programme.in | html %]</td>
      <td>[% programme.out | html %]</td>
      <td>[% programme.when | html %]</td>
      <td>[% programme.notes | html %]</td>
    </tr>

  [% END %]
  </table>
</div>
