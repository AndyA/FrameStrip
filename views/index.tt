<header class="header">
  <h1>[% title | html %]</h1>
</header>

<!--
  "state"           : "pending",
  "redux_reference" : "10001271397364161283",
  "programme_name"  : "ROCK PROFILES",
  "description"     : "Spoof music profiles series : ABBA :TOM AND SHIRLEY",
